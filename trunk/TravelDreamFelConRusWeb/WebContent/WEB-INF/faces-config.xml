<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
<navigation-rule>
       <from-view-id>packageManagement.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>sel</from-outcome>
           <to-view-id>
            
           /hotelManagement.xhtml
          
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>packageManagement.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>packageEdit</from-outcome>
           <to-view-id>
           /packageEdit.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
        <navigation-rule>
       <from-view-id>packageManagement.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>notBusy</from-outcome>
           <to-view-id>
          /packageEdit.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>packageManagement.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>errorBusy</from-outcome>
           <to-view-id>
           /packageErrorBusy.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>packageEdit.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>closed</from-outcome>
           <to-view-id>
           /packageManagement.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    
        <navigation-rule>
       <from-view-id>packageEdit.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>notBusyStage</from-outcome>
           <to-view-id>
          /stageManagement.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>packageEdit.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>errorBusyStage</from-outcome>
           <to-view-id>
           /stageErrorBusy.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    <navigation-rule>
       <from-view-id>stageManagement.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>closed</from-outcome>
           <to-view-id>
           /packageEdit.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
       <from-view-id>stageManagement.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>addedStage</from-outcome>
           <to-view-id>
           /packageEdit.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
       <from-view-id>stageManagement.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>inconsistentStage</from-outcome>
           <to-view-id>
           /stageErrorInconsistent.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
       </navigation-rule>
    
       
     <navigation-rule>
       <from-view-id>stageManagement.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>inconsistentDateStage</from-outcome>
           <to-view-id>
           /stageErrorDate.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
       
       
    </navigation-rule>
     <navigation-rule>
       <from-view-id>index.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>viewGift</from-outcome>
           <to-view-id>
           /giftList.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
       <from-view-id>index.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>codeError</from-outcome>
           <to-view-id>
           /giftListErrorCode.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
       <from-view-id>packageEdit.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>modifySuccess</from-outcome>
           <to-view-id>
           /packageModifiedPackageEmployee.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    
     <navigation-rule>
       <from-view-id>packageEdit.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>insertSuccess</from-outcome>
           <to-view-id>
           /packageInsertedSuccesEmployee.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
      </navigation-rule> 
        <navigation-rule>
       <from-view-id>packageEdit.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>inconsistentPackage</from-outcome>
           <to-view-id>
           /packageInconsistent.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
      
    </navigation-rule>
    
    
     <navigation-rule>
       <from-view-id>packageEdit.xhtml</from-view-id>
       <navigation-case>
           <from-outcome>errorBusyStage</from-outcome>
           <to-view-id>
           /stageErrorBusy.xhtml
           </to-view-id>
           <redirect/>  
       </navigation-case>
    </navigation-rule>
    
    

</faces-config>
